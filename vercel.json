{
    "version" : 2,
    "builds" :[
        {
            "src": "greatkart/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.11"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static",
            "config": {
                "distDir": "static"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "greatkart/wsgi.py"
        }
    ]
}